<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ajeshpai.androidanimatios">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".PhysicsBasedAnimations.SpringAnimationActivity" />
        <activity android:name=".PhysicsBasedAnimations.FlingActivity" />
        <activity android:name=".PhysicsBasedAnimations.TranslateAndRotateSpringAnimation" />
        <activity android:name=".PhysicsBasedAnimations.ChainedSpringAnimationActivity" />
        <activity android:name=".Transitions.FirstViewActivity" />
        <activity
            android:name=".Transitions.HomeView_AlarmViewSwitching"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        </activity>
        <activity android:name=".Transitions.SearchViewTransitionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Transitions.Search_Activity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
    </application>

</manifest>